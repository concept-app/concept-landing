{
  "functions": {
    "api/v1/rework/oauth/[...path].js": {
      "includeFiles": "server/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/v1/rework/oauth/:path*",
      "destination": "/api/v1/rework/oauth/[...path]"
    }
  ],
  "headers": [
    {
      "source": "/api/v1/rework/oauth/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
